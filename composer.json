{
    "name": "basel/repository-pipeline",
    "description": "Package to create repositories for each model and using pipeline in filters",
    "type": "composer-plugin",
    "keywords": [
        "image",
        "gd",
        "imagick",
        "laravel",
        "watermark",
        "thumbnail"
    ],
    "authors": [
        {
            "name": "baselaly",
            "email": "basel.3ly@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Basel\\RepositoryPipeline\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Basel\\RepositoryPipeline\\RepositoryPipelineProvider"
            ]
        }
    }
}